                "files"
            ]
        },
        "email": {
            "type": "email",
            "required": true
        },
        "expertise": {
            "type": "blocks",
            "required": false
        },
        "phone": {
            "type": "string"
        },
        "medicines": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::medicine.medicine",
            "mappedBy": "Added_by"
        }
    }
